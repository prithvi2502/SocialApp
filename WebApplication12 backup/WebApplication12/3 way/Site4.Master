    #menubar ul 
    {
        height: 350px;
	    margin: 20px 0 0 0;
	    _margin: 0; /*IE6 only*/
	    opacity: 0;
	    visibility: hidden;
	    position: absolute;
	    top: 38px;
	    left: 0;
	    z-index: 1;    
